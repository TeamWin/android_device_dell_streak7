# mount point   fstype          device
/recovery       emmc            /dev/block/platform/sdhci-tegra.3/by-name/SOS
/boot           emmc            /dev/block/platform/sdhci-tegra.3/by-name/LNX
/system         ext4            /dev/block/platform/sdhci-tegra.3/by-name/APP
/cache          ext3            /dev/block/platform/sdhci-tegra.3/by-name/CAC
/misc           emmc            /dev/block/platform/sdhci-tegra.3/by-name/MSC
/data           ext3            /dev/block/platform/sdhci-tegra.3/by-name/UDA length=-32768
/internal_sd	vfat        	/dev/block/platform/sdhci-tegra.3/by-name/SDC flags=display="Internal SDcard";storage;wipeingui
/sdcard 	vfat		/dev/block/platform/sdhci-tegra.2/mmcblk2p1   flags=display="Micro SDcard";storage;wipeingui;removable
